{
  "name": "express-ts-crud",
  "version": "1.0.0",
  "description": "CRUD API with Express, TypeScript, and SQLite",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "seed:users": "ts-node src/scripts/seedUsers.ts",
    "seed:posts": "ts-node src/scripts/seedPosts.ts",
    "seed": "yarn seed:users && yarn seed:posts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^14.14.31",
    "@types/sequelize": "^4.28.10",
    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.1.3"
  }
}
